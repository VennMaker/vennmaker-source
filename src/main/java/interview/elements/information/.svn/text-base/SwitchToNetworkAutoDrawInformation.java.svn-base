package interview.elements.information;

import interview.InterviewElementInformation;

public class SwitchToNetworkAutoDrawInformation extends InterviewElementInformation
{
	/**
	 * Network in Combobox
	 */
	private String network;
	
	public SwitchToNetworkAutoDrawInformation(String network)
	{
		this.network = network;
	}
	
	/**
	 * Returns the Network in the combobox of SwitchToNetworkElement
	 * @return  Network in the combobox of SwitchToNetworkElement
	 */
	public String getNetworkToSwitch()
	{
		return network;
	}
}
