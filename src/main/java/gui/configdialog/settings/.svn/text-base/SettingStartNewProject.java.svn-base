package gui.configdialog.settings;

import data.VennMakerActions;

/**
 * This Setting forces the reset of <code>VennMaker</code> to a new
 * <code>Project</code> if the <code>ConfigDialog</code> was cancled after
 * loading a template
 * 
 * 
 * 
 */
public class SettingStartNewProject implements ImmidiateConfigDialogSetting
{

	@Override
	public void set()
	{
		// TODO Auto-generated method stub
	}

	@Override
	public void undo()
	{
		VennMakerActions.forceNewProject();
	}

}
