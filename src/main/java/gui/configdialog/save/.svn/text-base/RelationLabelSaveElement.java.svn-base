package gui.configdialog.save;

import java.util.List;

import data.AttributeType;


/**
 * Objects of this class contain all necessary information to save
 * the selection of relation labels and tooltips
 * 
 * 
 *
 */
public class RelationLabelSaveElement extends SaveElement
{
	private List<AttributeType> labelList;
	private List<AttributeType> tooltipList;
	
	
	public RelationLabelSaveElement(List<AttributeType> labelList,List<AttributeType> tooltipList)
	{
		this.labelList = labelList;
		this.tooltipList = tooltipList;
	}

	/**
	 * @return the tooltipList
	 */
	public List<AttributeType> getTooltipList()
	{
		return tooltipList;
	}

	/**
	 * @return the labelList
	 */
	public List<AttributeType> getLabelList()
	{
		return labelList;
	}
}
